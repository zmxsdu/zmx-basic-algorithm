//线性筛
const int _=2e6+7;
int pri[_/10];
bool nonpri[_];
long long cnt=0,num[_];
void ini(){
	for(int i=2;i<_;i++){
		if(nonpri[i]) continue;
		pri[cnt++]=i;
		for(int j=i*i;j<_;j+=i){
			nonpri[j]=1;
		}
	}
} 


Python
MX = 1000
P = [0]  # 哨兵，避免二分越界
is_prime = [True] * MX
for i in range(2, MX):
    if is_prime[i]:
        P.append(i)  # 预处理质数列表
        for j in range(i * i, MX, i):
            is_prime[j] = False
