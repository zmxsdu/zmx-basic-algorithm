//线性筛
const int N=1e5+7;
int pri[N/5];
bool nonpri[N];
long long cnt=0,num[N];
int init = [](){
    for(int i=2;i<N;i++){
		if(nonpri[i]) continue;
		pri[cnt++]=i;
		for(long long j=1ll*i*i;j<1ll*N;j+=i){
			nonpri[j]=1;
		}
	}
    return 0;
}();


Python
MX = 1000
P = [0]  # 哨兵，避免二分越界
is_prime = [True] * MX
for i in range(2, MX):
    if is_prime[i]:
        P.append(i)  # 预处理质数列表
        for j in range(i * i, MX, i):
            is_prime[j] = False
